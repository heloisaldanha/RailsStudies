<div class="row">
    <div class="col-xs-12">
        <h1>Editar contato</h1>
    </div>

    <div class="col-md-6 col-md-offset-3 col-xs-12">
        <%= form_for @contact do |f| %>
            <%= render 'form', f: f %>

            <%= f.submit 'Atuallizar', class: 'btn btn-primary' %>
        <% end %>
    </div>
</div>